#include<iostream>
#include<vector>
#include<algorithm>
using namespace std;
int main()
{//	vector<data_type> var_name={val1, val2, ....., valn};
	int arr[]={5,2,4,3,1};
	vector<int> v(arr, arr+5);
	int c;
	sort(v.begin(), v.end());
	cout<<"\n\nVector elements: ";
	for(c=0; c<5; c++)
		cout<<v[c]<<" ";
	
	return 0;	
}
